{
  "batch_shipyard": {
    "storage_account_settings": "miscellaneus"
  },
  "data_replication":{
    "peer_to_peer":{
       "enabled":true ,
       "compression":true,
       "concurrent_source_downloads": 10,
       "direct_download_seed_bias": 20
    },
    "non_peer_to_peer_concurrent_downloading": true
  },
  "global_resources": {
    "docker_images": [
      "VALUE_WILL_BE_GENERATED"
    ],
    "docker_volumes": {
      "shared_data_volumes": {
        "g4dbsvol": {
          "volume_driver": "azurefile",
          "storage_account_settings": "g4databases",
          "azure_file_share_name": "g4db",
          "container_path": "/usr/local/geant4/data",
          "mount_options": [
            "filemode=0744",
            "dirmode=0744",
            "nolock=true"
          ]
        }
      },
      "data_volumes": {
        "output": {
          "host_path": "$AZ_BATCH_TASK_WORKING_DIR",
          "container_path": "/output"
        }
      }
    }
  }
}
